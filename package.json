    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/node": "^18.18.5",
    "@types/node-fetch": "^2.6.6",
    "@types/rss": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8"
